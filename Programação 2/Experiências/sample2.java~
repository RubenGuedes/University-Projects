import java.lang.*
import javax.swing.*
  
  class sample2{
  
  public static void main(String[] args){
  Scanner scanner = new Scanner(System.in);
  System.out.println("Temperatura em C");
  scanner = nextDouble();
  double f = 1.8*scanner+32;
  System.out.println("Temperatura em F: " f);
  }
}
    
    
  